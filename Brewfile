
# tap repositories and their packages

## caskroom/cask
tap caskroom/cask
install brew-cask

## homebrew/dupes
tap homebrew/dupes
install zlib

## josegonzalez/php
tap josegonzalez/php
install php55 --without-apache

## marcqualie/nginx
tap marcqualie/nginx
install nginx-full --with-sub

## rcmdnk/file
tap rcmdnk/file
install brew-file

